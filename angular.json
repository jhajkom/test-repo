{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "sop-form-viewer": {
      "root": "projects/sop-form-viewer",
      "sourceRoot": "projects/sop-form-viewer/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/sop-form-viewer/tsconfig.lib.json",
            "project": "projects/sop-form-viewer/ng-package.json"
          }
        }
      }
    }
  },
  "defaultProject": "sop-form-viewer"
}
